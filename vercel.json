{
  "version": 2,
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/build",
  "functions": {
    "api/contact.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    {
      "source": "/api/contact",
      "destination": "/api/contact"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}